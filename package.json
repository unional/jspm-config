{
  "name": "jspm-config",
  "version": "0.0.0",
  "description": "Read jspm config and such",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "dependencies": {
    "any-promise": "^1.3.0",
    "blue-tape-fixture": "^0.1.0",
    "deep-extend": "^0.4.1",
    "graceful-fs": "^4.1.4",
    "object.pick": "^1.1.2",
    "parse-json": "^2.2.0",
    "strip-bom": "^3.0.0",
    "thenify": "^3.2.0",
    "throat": "^3.0.0",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "blue-tape": "^0.2.0",
    "jspm": "^0.17.0-beta.22",
    "onchange": "^2.5.0",
    "tap-spec": "^4.1.1",
    "ts-node": "^0.9.1",
    "tslint": "^3.11.0",
    "tslint-config-unional": "^0.2.7",
    "typescript": "^1.9.0-dev.20160618-1.0",
    "typings": "^1.3.0"
  },
  "scripts": {
    "build": "echo building... && tsc -d -p tsconfig.build.json",
    "lint": "echo linting... && tslint \"src/**/*.ts\"",
    "test": "echo testing... && cd test && ts-node ../node_modules/blue-tape/bin/blue-tape \"**/*.ts\" | tap-spec",
    "watch": "onchange -w \"**/*.ts\" -i -e \"dist/**\" -- npm -s run lint+test",
    "verify": "npm run build && npm -s run lint+test",
    "build+test": "npm run build && npm test",
    "lint+test": "npm run lint && npm test",
    "preversion": "npm run verify",
    "prepublish": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unional/jspm-config.git"
  },
  "keywords": [
    "jspm",
    "systemjs"
  ],
  "author": "Unional <homawong@gmail.com> (http://github.com/unional)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unional/jspm-config/issues"
  },
  "homepage": "https://github.com/unional/jspm-config#readme"
}
