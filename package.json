{
  "name": "jspm-config",
  "version": "0.1.3",
  "description": "Read jspm config and such",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/",
    "typings.json"
  ],
  "dependencies": {
    "any-promise": "^1.3.0",
    "deep-extend": "^0.4.1",
    "graceful-fs": "^4.1.4",
    "make-error-cause": "^1.2.1",
    "object.pick": "^1.1.2",
    "parse-json": "^2.2.0",
    "strip-bom": "^3.0.0",
    "thenify": "^3.2.0",
    "throat": "^3.0.0",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "ava-fixture": "^0.1.5",
    "jspm": "^0.17.0-beta.22",
    "onchange": "^2.5.0",
    "rimraf": "^2.5.3",
    "tap-spec": "^4.1.1",
    "ts-node": "^0.9.3",
    "tslint": "^3.13.0",
    "tslint-config-unional": "^0.4.3",
    "typescript": "^2.0.0",
    "typings": "^1.3.1"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "rimraf dist && tsc && ava \"dist/**/*.spec.js\"",
    "watch": "node scripts/watch.js",
    "verify": "npm run test+lint && npm run build",
    "test+lint": "npm test && npm run lint",
    "preversion": "npm run verify",
    "prepublish": "typings install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unional/jspm-config.git"
  },
  "keywords": [
    "jspm",
    "systemjs"
  ],
  "author": "Unional <homawong@gmail.com> (http://github.com/unional)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unional/jspm-config/issues"
  },
  "homepage": "https://github.com/unional/jspm-config#readme"
}
